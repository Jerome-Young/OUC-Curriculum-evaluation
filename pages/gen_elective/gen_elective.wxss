/* pages/gen_elective/gen_elective.wxss */
page{
 /* background-color: cadetblue;
  background-image: url(../../../image/weixin_logo.png);*/
  background-repeat:repeat-x;
  background-position-y: top;
}
.demo-view-2{
  background-color: rgb(0, 0, 0);
  height: 450rpx;
  display: flex;
  align-items: left;
  justify-content: center;
}
.search-bar {
  position: relative;
  padding: 8px 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  background-color: #EFEFF4;
  border-top: 1rpx solid #D7D6DC;
  border-bottom: 1rpx solid #D7D6DC;
}
.search_in-box {
  position: absolute;
  left: 10px;
  top: 7px;
}
.search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 5px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.search-bar__box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}
.search-bar__input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}
.search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09BB07;
  white-space: nowrap;
}
.border-4{
    text-align: center;
    margin: 10rpx;
  
    border: double #000000 10rpx;
}
.border-8{
    text-align: center;
    margin: 20rpx;
    border: outset #48a0f3 10rpx;
}
.cent{
  text-align: center;
  margin: 30rpx;
}
/* 返回顶部 */
.goTop{
  height: 80rpx;
  width: 80rpx;
  position: fixed;
  bottom: 50rpx;
  background: rgba(0,0,0,.3);
  right: 30rpx;
  border-radius: 50%;
}