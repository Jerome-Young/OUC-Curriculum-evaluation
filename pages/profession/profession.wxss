.divider{
  background:#f5f5f5;
  height: 80rpx;
  width:100%;
}
.tab {
  display: flex;
  flex-direction: column;
  }
.tab-nav {
  border-bottom: 1rpx solid #ddd;
  width: 100%;
  height: 80rpx;
  display: flex;
  line-height: 79rpx;
  position: fixed;
  }
  .tab-line {
  position: absolute;
  left: 0;
  bottom: -1rpx;
  height: 5rpx;
  background: #f7982a;
  transition: all 0.3s;
  }
.tab-content {
  flex: 1;
  overflow-y: auto;
  overflow-x: hidden;
  }
  /*选项卡页面联动切换*/
.swiper-tab {
  width: 100%;
  border-bottom: 2rpx solid #777;
  text-align: center;
  line-height: 80rpx;
  }
.swiper-tab-list {
  font-size: 30rpx;
  display: inline-block;
  width: 33.33%;
  color: #777;
  }
.on {
  color: #da7c0c;
  border-bottom: 1rpx solid #da7c0c;
  }
.swiper-box {
  display: block;
  height: 100%;
  width: 100%;
  overflow: hidden;
  }
.swiper-box view {
  text-align: center;
  } 
  .text{
    border: double #000 10rpx;
  }
  .left-text{
    display: inline-block;
    text-align: left;
    padding-left: 10px;
    height: 80px;
    font-size: 16px;
    font-weight: bolder;
  }
  .teacher-text{
    font-size: 14px;
    font-weight: 500;
    margin-right: 200px;
  }
  .right-text{
    display: inline-block;
    text-align: right;
  }
  .score-text{
    font-size: 14px;
    font-weight: 500;
  }
  
  